{
    "name": "tiny-pixel/server-side-rendering",
    "description": "Server side rendering JavaScript in your Acorn application",
    "homepage": "https://github.com/pixelcollective/laravel-server-side-rendering",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "symfony/process": "^3.0|^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "TinyPixel\\SSR\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
      "acorn": {
        "providers": [
          "TinyPixel\\SSR\\Providers\\SSRServiceProvider"
        ]
      }
    }
}